{
  "name": "@nayan-ui/react",
  "private": false,
  "sideEffects": false,
  "description": "React Component Library for smooth and faster web application development.",
  "version": "1.0.0",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://www.nayanui.com",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "default": "./dist/index.es.js"
    },
    "./styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && vite build && tsc -p tsconfig.build.json",
    "dev": "rimraf dist && vite build --watch"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "react-hook-form": "^7.60.0",
    "react-linkify": "^1.0.0-alpha",
    "react-select": "^5.10.2",
    "tailwind-merge": "^3.3.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ursnj/nayan-ui"
  },
  "keywords": [
    "react-js",
    "react-component-library",
    "react-reusable-components",
    "react-ui-library",
    "next-js-components",
    "react-vite",
    "dev-tools",
    "seo-tools",
    "nayan-ui"
  ],
  "author": {
    "name": "Niranjan Devasani",
    "email": "niranjan.devasani@gmail.com",
    "url": "https://www.ursnj.com"
  },
  "bugs": {
    "url": "https://github.com/ursnj/nayan-ui/issues"
  },
  "license": "MIT"
}
